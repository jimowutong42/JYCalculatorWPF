<UserControl x:Class="JX3PZ.Views.EquipShowStrengthStar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:JX3PZ.Views"
             xmlns:hc="https://handyorg.github.io/handycontrol"
             xmlns:sf="http://schemas.syncfusion.com/wpf"
             mc:Ignorable="d">
    <RichTextBox Style="{StaticResource Style.RichTextBlockBox}"
                 MaxWidth="400">
        <FlowDocument local:FlowDocumentPagePadding.PagePadding="0">
            <Paragraph>
                <Run Foreground="Yellow" Text="{Binding StrengthStar1, Mode=OneWay}" /><Run Foreground="White" Text="{Binding StrengthStar2, Mode=OneWay}" />
            </Paragraph>
        </FlowDocument>
    </RichTextBox>
</UserControl>